<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	 http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="pro" class="com.spring.five.DIformofObjects.ProductBean"> 
		<constructor-arg value="11"/>
		<constructor-arg value="I phone"/>
		<constructor-arg value="99999.00"/>
		<constructor-arg value="1"/>
	</bean>
	<bean id="order" class="com.spring.five.DIformofObjects.OrderBean"> 
		<constructor-arg value="1"/>
		<constructor-arg ref="pro"/>
	</bean>
</beans>